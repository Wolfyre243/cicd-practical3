{
  "name": "api",
  "version": "1.0.0",
  "description": "Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:test": "NODE_ENV=test PORT=5175 node dist/index.js",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:test": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/testdb\" prisma migrate deploy",
    "db:seed:dev": "ts-node -r dotenv/config prisma/seed.ts",
    "db:seed:off": "ts-node -r dotenv/config prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config prisma/seed.ts"
  },
  "keywords": [
    "express",
    "prisma",
    "typescript",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.39.1",
    "globals": "^14.0.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  }
}
